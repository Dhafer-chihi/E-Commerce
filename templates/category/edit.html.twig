{% extends "base.html.twig" %}
{% block title %}
    Modifier : {{category.name}}
{% endblock  %}
{% block body %}
    <h1>Modifier la categorie : {{category.name}}</h1>

    {{form_start(formView)}}
    {{form_errors(formView)}}
    {{form_widget(formView)}}
    <button type="submit" class="btn btn-primary">
        <i class="fas fa-save"></i>
        Modifier Categorie
    </button>
    {{form_end(formView)}}
{% endblock  %}